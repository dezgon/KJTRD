@model IEnumerable<QUEJATERD.DBModels.PostActions>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.IsPositive)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CreatedAt)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DeletedAt)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PostActionTypes.ActionTypeName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Posts.Title)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Users.Surname)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Users1.Surname)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.IsPositive)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CreatedAt)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DeletedAt)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PostActionTypes.ActionTypeName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Posts.Title)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Users.Surname)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Users1.Surname)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
